<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
      http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<navigation-rule>
		<navigation-case>
			<from-outcome>goToHome</from-outcome>
			<to-view-id>/pages/home.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>goToNuovaVenditaDopoSave</from-outcome>
			<to-view-id>/pages/vendita/AddVendita.xhtml</to-view-id>			
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>goToClienteMod</from-outcome>
			<to-view-id>/pages/cliente/ModCliente.xhtml</to-view-id>			
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>goToClienteSearch</from-outcome>
			<to-view-id>/pages/cliente/SearchCliente.xhtml</to-view-id>			
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>goToVendita</from-outcome>
			<to-view-id>/pages/vendita/AddVenditaSearch.xhtml</to-view-id>			
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>goToAtto</from-outcome>
			<to-view-id>/pages/vendita/SearchVendita.xhtml</to-view-id>			
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>goToStampaAtto</from-outcome>
			<to-view-id>/pages/vendita/StampaAtto.xhtml</to-view-id>			
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>goToBancometalliMod</from-outcome>
			<to-view-id>/pages/bancometalli/ModBancometalli.xhtml</to-view-id>			
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>goToBancometalliSearch</from-outcome>
			<to-view-id>/pages/bancometalli/SearchBancometalli.xhtml</to-view-id>			
		</navigation-case>
	</navigation-rule>
	
	<lifecycle>
  		<phase-listener>it.goldmanager.common.MultiPageMessagesSupport</phase-listener>  
	</lifecycle>
	
	
	<application>
		<locale-config>
			<default-locale>it</default-locale>
			<supported-locale>it_IT</supported-locale>
		</locale-config>
	</application>
	<application>
		<resource-bundle>
			<base-name>Messages</base-name>
			<var>mex</var>
		</resource-bundle>
	</application>

</faces-config>
